<h2 id="page_title">Stuff I 've written</h2>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @posts.each do |post| %>
    <div class="post">
      <p class="date"><%= post.created_at.strftime("%A, %B %d") %></p>
      <h5><%= link_to post.title, post %></a></h5>
      <hr>
    </div>
  <% end %>
</div>

<div class="pagination">
  <span class="previous_page disable">Previous</span>
  <em class="current">1</em>
  <a rel="next" href="#">2</a>
  <a class="next_page" href="#">Next</a>
</div>
